{
    "name": "Getting Started Example",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "https://*.panopto.com/Panopto/*/Viewer*"
            ],
            "js": [
                "src/scripts/main.js"
            ],
            "css": ["src/styles/layout.css"],
            "run_at": "document_end"
        }
    ]
    
}